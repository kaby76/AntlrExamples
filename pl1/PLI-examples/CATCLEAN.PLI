1DECLARE  BUFFER  CHAR(140) STATIC INIT(' ');
 /*                    FORMAT 1 DSCB                                 */
 DECLARE
     1 IECSDSL1 BASED(ADDR(BUFFER)),    /* START OF THE FORMAT 1 DSCB*/
         2 DS1DSNAM    CHAR(44),        /* DATA SET NAME             */
         2 DS1FMTID    CHAR(1),         /* FORMAT IDENTIFIER         */
         2 DS1DSSN     CHAR(6),         /* DATA SET SERIAL NUMBER    */
