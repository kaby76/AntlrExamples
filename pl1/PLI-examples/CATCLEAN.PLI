1        2 DS1RECFM    CHAR(1),         /* RECORD FORMAT             */
         2 DS1OPTCD    CHAR(1),         /* OPTION CODE               */
         2 DS1BLKL     CHAR(2),         /* BLOCK LENGTH              */
         2 DS1LRECL    CHAR(2),         /* RECORD LENGTH             */
