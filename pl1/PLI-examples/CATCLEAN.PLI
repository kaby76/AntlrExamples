          3 DS1DSRG2,                   /* SECOND BYTE OF DS1DSORG   */
           4 DS1DSGGS BIT(1),           /* GS - GRAPHICS
                                            ORGANIZATION             */
           4 DS1DSGTX BIT(1),           /* TX - TCAM LINE GROUP      */
           4 DS1DSGTQ BIT(1),           /* TQ - TCAM MESSAGE QUEUE   */
